pipeline{
    agent any 
    stages {
         stage('Check version'){
             steps {
                 echo "Stage 1 - Step 1"
                 sh 'mvn --version'
             }
         }
          stage('Maven install'){
             steps{
                 echo "Stage 2 - Step 1"
                 sh 'mvn install'
             }
             
         }
          stage('Compile and Package'){
             steps{
                 echo "Stage 2 - Step 1"
                 sh 'mvn clean package'
             }
             
         }
         
    }
}
